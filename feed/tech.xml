---
layout: none
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>ChrisPliakas.com - Tech</title>
    <link href="{{ site.url }}"/>
    <link type="application/atom+xml" rel="self" href="{{ site.url }}/feed/tech.xml"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>{{ site.url }}/feed/tech.xml</id>
    <author>
        <name>Chris Pliakas</name>
        <email>opensource@chrispliakas.com</email>
    </author>

    {% for post in site.tags.tech %}
    <entry>
        <id>{{ site.url }}{{ post.id }}</id>
        <link type="text/html" rel="alternate" href="{{ site.url }}{{ post.url }}"/>
        <title>{{ post.title }}</title>
        <published>{{ post.date | date_to_xmlschema }}</published>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <author>
            <name>Chris Pliakas</name>
            <email>opensource@chrispliakas.com</email>
        </author>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}
</feed>
